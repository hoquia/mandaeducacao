<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Emolumento.
    -->
    <changeSet id="20230904002308-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="categoria_id"
                                 baseTableName="emolumento"
                                 constraintName="fk_emolumento__categoria_id"
                                 referencedColumnNames="id"
                                 referencedTableName="categoria_emolumento"/>

        <addForeignKeyConstraint baseColumnNames="imposto_id"
                                 baseTableName="emolumento"
                                 constraintName="fk_emolumento__imposto_id"
                                 referencedColumnNames="id"
                                 referencedTableName="imposto"/>

        <addForeignKeyConstraint baseColumnNames="referencia_id"
                                 baseTableName="emolumento"
                                 constraintName="fk_emolumento__referencia_id"
                                 referencedColumnNames="id"
                                 referencedTableName="emolumento"/>

        <addForeignKeyConstraint baseColumnNames="plano_multa_id"
                                 baseTableName="emolumento"
                                 constraintName="fk_emolumento__plano_multa_id"
                                 referencedColumnNames="id"
                                 referencedTableName="plano_multa"/>
    </changeSet>
</databaseChangeLog>
