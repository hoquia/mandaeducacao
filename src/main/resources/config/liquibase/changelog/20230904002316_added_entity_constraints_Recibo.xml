<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Recibo.
    -->
    <changeSet id="20230904002316-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="utilizador_id"
                                 baseTableName="recibo"
                                 constraintName="fk_recibo__utilizador_id"
                                 referencedColumnNames="id"
                                 referencedTableName="app_user"/>

        <addForeignKeyConstraint baseColumnNames="matricula_id"
                                 baseTableName="recibo"
                                 constraintName="fk_recibo__matricula_id"
                                 referencedColumnNames="id"
                                 referencedTableName="matricula"/>

        <addForeignKeyConstraint baseColumnNames="documento_comercial_id"
                                 baseTableName="recibo"
                                 constraintName="fk_recibo__documento_comercial_id"
                                 referencedColumnNames="id"
                                 referencedTableName="documento_comercial"/>

        <addForeignKeyConstraint baseColumnNames="transacao_id"
                                 baseTableName="recibo"
                                 constraintName="fk_recibo__transacao_id"
                                 referencedColumnNames="id"
                                 referencedTableName="transacao"/>
    </changeSet>
</databaseChangeLog>
